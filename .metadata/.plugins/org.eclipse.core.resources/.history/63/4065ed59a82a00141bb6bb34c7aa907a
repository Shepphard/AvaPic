package mp.edu.hs.trier.de.controller.parameters;

import mp.edu.hs.trier.de.model.ShapeModel;

public class RandomizeController {

	private ShapeModel head;
	private ShapeModel body;
	private ShapeModel legs;
	
	public RandomizeController(ShapeModel head, ShapeModel body, ShapeModel legs)
	{
		this.head = head;
		this.body = body;
		this.legs = legs;
	}
	
	public void randomize()
	{
		head.randomize();
		body.randomize();
		legs.randomize();
	}
}
